{
    "manifest_version": 2,
    "name": "Tohru-sama",
    "description": "Minimal TODO App made with Angular",
    "short_name": "Tohru",
    "version": "0.1",
    "browser_action":{
        "default_popup": "index.html",
        "default_icon": "images/logo.png"
    },
    "permissions": [
        "debugger",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 
}
